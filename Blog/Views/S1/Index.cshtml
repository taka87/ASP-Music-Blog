@model IEnumerable<Blog.Models.S1>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<style>
    body {
        background-image: url('../../Content/srybsko/background/Mile_Kitic_2001_Prednja.jpg');
        background-repeat: no-repeat;
        background-size: cover;
        color: aqua;
    }
</style>
